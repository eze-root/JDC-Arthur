config singbox 'main'
	option enabled '0'
	option sbox_dir '/etc/sbox'
	option bin_path '/etc/sbox/sing-box'
	option config_path '/etc/sbox/config.json'
	option bin_url ''
	option config_url ''
	option check_url 'https://www.baidu.com'
	option tproxy_port '9898'
	option fwmark '1'
	option route_table '100'
	option lan_ifnames 'br-lan'
	option proxy_router '0'
	option respawn_threshold '3600'
	option respawn_timeout '5'
	option respawn_retry '5'
